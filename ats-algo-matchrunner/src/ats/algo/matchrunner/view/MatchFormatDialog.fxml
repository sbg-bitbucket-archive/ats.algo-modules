<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="403.0" prefWidth="656.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ats.algo.matchrunner.view.MatchFormatDialogController">
   <children>
      <TableView fx:id="matchFormatTable" layoutY="50.0" prefHeight="261.0" prefWidth="656.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="389.5" text="Element" />
          <TableColumn fx:id="valueColumn" prefWidth="265.0" text="Value" />
        </columns>
      </TableView>
      <Label layoutX="161.0" layoutY="322.0" prefHeight="21.0" prefWidth="488.0" text="If not replaying from file then edit any values above that need changing" />
      <Button layoutX="571.0" layoutY="352.0" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
      <Button layoutX="514.0" layoutY="352.0" mnemonicParsing="false" onAction="#handleOk" text="OK" />
      <Label fx:id="errorMessage" layoutX="36.0" layoutY="14.0" prefHeight="21.0" prefWidth="541.0" />
      <Button layoutX="21.0" layoutY="352.0" mnemonicParsing="false" onAction="#handleReplay" prefHeight="31.0" prefWidth="148.0" text="Replay from file..." />
      <RadioButton fx:id="externalModel" layoutX="344.0" layoutY="359.0" mnemonicParsing="false" text="Use external model" />
   </children>
</AnchorPane>
