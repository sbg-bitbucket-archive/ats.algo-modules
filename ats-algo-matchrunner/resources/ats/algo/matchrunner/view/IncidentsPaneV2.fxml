<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="346.0" prefWidth="201.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ats.algo.matchrunner.view.IncidentsPaneV2Controller">
   <children>    
      <Label fx:id="labelTitle" alignment="CENTER" prefHeight="28.0" prefWidth="200.0" text="Match Incidents" textAlignment="RIGHT">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Button fx:id="buttonUndo" layoutX="4.0" layoutY="272.0" mnemonicParsing="false" onAction="#onUndoLastButtonPressed" text="[U]ndo last" />
      <Line endX="200.0" />
      <ListView fx:id="observableList1" layoutX="4.0" layoutY="99.0" prefHeight="133.0" prefWidth="120.0" />
      <Button fx:id="buttonSendIncident" layoutX="4.0" layoutY="236.0" mnemonicParsing="false" onAction="#onSendIncidentButtonPressed" text="[G]enerate Incident" />
      <ComboBox fx:id="observableSources" layoutX="16.0" layoutY="41.0" prefWidth="150.0" />
      <Label layoutX="16.0" layoutY="22.0" text="Incident source" />
      <ListView fx:id="observableList2" layoutX="139.0" layoutY="100.0" prefHeight="56.0" prefWidth="58.0" />
      <Label fx:id="labelList1" layoutX="4.0" layoutY="78.0" text="Incident type" />
      <Label fx:id="labelList2" layoutX="136.0" layoutY="78.0" text="Team" />
      <Button fx:id="buttonMore" layoutX="6.0" layoutY="308.0" mnemonicParsing="false" onAction="#onMoreButtonPressed" prefHeight="31.0" prefWidth="188.0" text="More options..." />
   </children>
</AnchorPane>
